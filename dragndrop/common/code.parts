Import('*')

env.PartName(env.subst('code_${TARGET_ARCH}'))

env.DependsOn([Component('common.headers')])

source = [env.SharedObject('src/' + x) for x in (
    'mycrt.c',
    'ddlock.cpp',
    'debug.cpp',
    'myunknown.cpp',
    'mywnd.cpp',
    'new.cpp',
    'shutils.cpp',
    'utils.cpp',
    )]

env.SdkLib(
    env.StaticLibrary(
        target='${PART_PARENT_NAME}_${TARGET_ARCH}', source=source
    )
)

